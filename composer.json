{
  "name": "rocketeers/rocketeer-laravel",
  "description": "Laravel integration for Rocketeer",
  "license": "MIT",
  "keywords": [
    "rocketeer",
    "laravel"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "anahkiasen/rocketeer": "3.0.*@dev",
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit": "^5.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Rocketeer\\Plugins\\Laravel\\": "src/Laravel"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rocketeer\\Plugins\\Laravel\\": "tests",
      "Rocketeer\\": "vendor/anahkiasen/rocketeer/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
